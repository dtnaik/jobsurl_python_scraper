# PostgreSQL Database Connection URL
# Example: postgresql://user:password@host:port/database
DATABASE_URL="YOUR_DATABASE_CONNECTION_STRING"

# Google Cloud Storage Configuration
GCS_BUCKET_NAME="solwizz"
GCS_PROJECT_ID="YOUR_GCP_PROJECT_ID"
# Path to your Google Cloud service account key file (JSON)
# Example: /path/to/your/gcp-keyfile.json or C:\path\to\your\gcp-keyfile.json
GOOGLE_APPLICATION_CREDENTIALS="PATH_TO_YOUR_GCP_KEYFILE.json"

# Optional: Specify the folder within the bucket
GCS_FOLDER_NAME="scraped_jobs"
